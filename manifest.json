{
   "background": {
      "scripts": [
         "js/config.js",
         "js/fn.js",
         "js/storage-local.js",
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon/icon-19.png",
         "38": "icon/icon-38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Time Keep"
   },
   "description": "Discover your browsing habits! Time tracking at its best.",
   "icons": {
      "128": "icon/icon-128.png",
      "16": "icon/icon-16.png",
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png"
   },
   "manifest_version": 2,
   "name": "Time Keep",
   "offline_enabled": true,
   "optional_permissions": [
      "\u003Call_urls>"
   ],
   "permissions": [
      "tabs",
      "idle",
      "storage",
      "unlimitedStorage"
   ],
   "version": "1.0.0"
}